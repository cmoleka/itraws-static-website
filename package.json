{
  "name": "itraws-website-2019",
  "version": "1.0.0",
  "description": "Itraws frontend website made 2019 with nuxtjs",
  "author": "Carlo Moleka",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.3.0",
    "@nuxtjs/markdownit": "^1.2.3",
    "@tryghost/content-api": "^1.0.0",
    "@vue/babel-preset-app": "^3.3.0",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.0-rc.11",
    "contentful": "^7.1.0",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "nuxt": "^2.3.4",
    "vue-particles": "^1.0.9"
  },
  "devDependencies": {
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.12.1",
    "eslint-plugin-vue": "^5.1.0",
    "node-sass": "^4.13.1",
    "nodemon": "^1.18.9",
    "sass-loader": "^7.1.0"
  }
}
